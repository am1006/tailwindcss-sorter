{
	"compilerOptions": {
		"module": "CommonJS",
		"target": "ES2022",
		"lib": [
			"ES2022"
		],
		"moduleResolution": "node",
		"esModuleInterop": true,
		"sourceMap": true,
		"rootDir": "src",
		"outDir": "out",
		"strict": true,
		"skipLibCheck": true
	},
	"exclude": [
		"node_modules",
		"dist"
	]
}
